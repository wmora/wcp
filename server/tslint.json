{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "trailing-comma": [
            "esSpecCompliant"
        ],
        "object-literal-sort-keys": [
            "match-declaration-order"
        ],
        "no-console": [
            false
        ],
        "interface-name": [
            false
        ]
    },
    "rulesDirectory": []
}
